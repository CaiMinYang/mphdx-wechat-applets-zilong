page{
  background: #F1F1F1;
}
.no-com {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.no-img {
  width: 164px;
  height: 133px;
  margin: 20px 0;
}

.no-text {
  height: 22px;
  font-size: 16px;
  font-weight: 400;
  color: #666666;
  line-height: 22px;
}

.list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 64px;
  background: #fff;
  margin-bottom: 10px;
  padding: 10px;
  box-sizing: border-box;
}

.left {
  display: flex;
}

.logo {
  width: 44px;
  height: 44px;
  margin-right: 12px;
}

.tx {
  display: flex;
  flex-direction: column;
}

.title {
  width: 180px;
  height: 22px;
  line-height: 22px;
  margin-bottom: 4px;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 16px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
}

.desc {
  width: 220px;
  height: 20px;
  line-height: 20px;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
}

.arrow {
  width: 18px;
  height: 18px;
}