  color: #333;
  box-sizing: border-box;
}

.icon-down image,
.icon-up .image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 20rpx;
}


.card-group-wrap {
  margin-top: 20rpx;
  border-top: 1px solid #e5e5e5;
  background: #fff;
}

.open-contact {
  border-bottom: 1px solid #e5e5e5;
}

.card-group-box {
  margin-left: 70rpx;
  padding: 20rpx 0 20rpx 0;
