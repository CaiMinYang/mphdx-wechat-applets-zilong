.detail-fields {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 120rpx;
  display: flex;
  flex-direction: column;
  width: 100%;
  padding-bottom: 64rpx;
  background: #FFFFFF;
  box-shadow: 0px 2px 16px 0px rgba(0, 0, 0, 0.2);
  border-radius: 0px 0px 16px 16px;
  overflow: hidden;
  z-index: 100;
  box-sizing: border-box;
}

.fields-box {
  height: auto;
  overflow-y: auto;
}

.detail-fields,
.fields-box {
  transition: height 1.1s linear;
}

.ishide {
  height: 0;
  padding: 0;
  overflow: hidden;
}

.detail-fields.ishide {
  bottom: auto;
}

.see-info {
  position: absolute;
  bottom: 0;
  left: 0;
  flex: none;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
