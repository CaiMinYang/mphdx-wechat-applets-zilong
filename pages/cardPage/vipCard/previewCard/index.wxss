  border-radius: 18px;
  cursor: pointer;
}

.vipCard {
  display: block;
  width: 100%;
  height: 380rpx;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.4);
}

.editCard {
  width: 750rpx;
  height: 88rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #FFFFFF;
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  line-height: 88rpx;
}

.to-chat {
  position: fixed;
  right: 30px;
  bottom: 70px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 58px;
  height: 58px;
  border-radius: 50%;
  background: #FFFFFF;
  box-shadow: 0px 1px 6px 0px rgba(0, 0, 0, 0.2);
  border: 2px solid #FFFFFF;
  z-index: 20;
}

.chat-text {
  font-size: 11px;
  font-weight: 400;
  color: #1195DD;
}

.vipItem {
  display: flex;
  justify-content: center;
  align-items: center;
}

.jiaoContainer {
  width: 100rpx;
  height: 100rpx;
