.info-item {
  padding-left: 30rpx;
  background: #fff;
  box-sizing: border-box;
}

.info-item:nth-child(4n):not(:last-child) {
  margin-bottom: 20rpx;
}

.info-item:nth-child(4n):not(:last-child) .info-item-inner,
.info-item:last-child .info-item-inner {
  border-bottom: none;
}

.info-item-inner {
  display: flex;
  align-items: center;
  min-height: 124rpx;
  padding: 12rpx 0;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
}

.inner-file{
  flex-direction: column;
}

.item-file-inner{
  align-items: center;
  min-height: 120rpx;
  display: flex;
  width: 100%;
  border-bottom: 1px solid #F1F1F1;
}

.first-letter {
  width: 24px;
  height: 24px;
  margin-right: 15px;
  background: #1195DD;
  border-radius: 4px;
  text-align: center;
  font-size: 14px;
  color: #fff;
  line-height: 24px;
  font-weight: 400;
}

.info-wrap {
  display: flex;
  flex-direction: column;
}

.info-title {
  height: 17px;
  margin-bottom: 5px;
  line-height: 17px;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 12px;
  font-weight: 400;
  color: #999999;
}

.file-title{
  height: 120rpx;
  line-height: 127rpx;
  font-size: 14px;
}

.info-desc {
  /* height: 20px; */
  line-height: 40rpx;
  max-width: 600rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}

.info-img {
  flex-direction: row;
  justify-content: space-between;
  flex: 1;
  padding-right: 20px;
  align-items: center;
}

.info-img .info-title {
  margin-bottom: 0;
}

.info-imgs {
  width: 36px;
  height: 36px;
}

.edit-wrap {
  height: 60px;
  background: #FFFFFF;
  padding: 10px 20px;
  box-sizing: border-box;
  border-top: 1px solid #f5f5f5;
}

.edit-btn {
  height: 40px;
  background: #1195DD;
  border-radius: 22px;
  border: none;
  line-height: 40px;
  font-size: 16px;
  font-weight: 400;
  color: #FFFFFF;
}

