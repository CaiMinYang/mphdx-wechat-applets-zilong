.tab-wrap {
  flex: 1;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
}

.tab-folder {
  flex: 1;
  height: 36px;
  margin: 0 20rpx 0 0;
  line-height: 36px;
  font-size: 16px;
  font-weight: 400;
  color: #FFFFFF;
  background: #ffffff;
  border: 2rpx solid #1195DD;
  border-radius: 18px;
}

.tab-create {
  flex: 1;
  height: 36px;
  margin: 0;
  line-height: 36px;
  font-size: 16px;
  font-weight: 400;
  color: #FFFFFF;
  background: #1195DD;
  border-radius: 18px;
}

.to-give-carte {
  width: 100%;
  height: 36px;
  line-height: 36px;
  background: #1195DD;
  font-weight: 400;
