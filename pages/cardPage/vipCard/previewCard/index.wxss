  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 80rpx;
}

image.dimensional-img {
  float: right;
  height: 60rpx !important;
  width: 60rpx !important;
  padding-right: 0 !important;
}

.img-wrap {
  display: flex;
  flex-direction: column;
  padding: 0 30rpx 0 0;
  margin-top: 20rpx;
  align-items: center;
  background-color: #fff;
}

.de-img {
  width: 100%;
}

.icon-up {
  text-align: center;
}

.leave-word-wrap {
  position: absolute;
  width: 100rpx;
  padding-bottom: 0px !important;
  right: 12px;
  text-align: center;
  font-size: 28rpx;
  color: #555
}

.no-cardlist {
  height: 100%;
  padding-top: 400rpx;
  font-size: 32rpx;
  text-align: center;
  color: #333;
  box-sizing: border-box;
}

.icon-down image,
.icon-up .image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 20rpx;
}

